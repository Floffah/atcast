{
    "name": "@atcast/atproto",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup --minify --clean",
        "dev": "tsup --watch",
        "format": "prettier --write **/*.{ts,js,json} src --ignore-path ../../.gitignore",
        "lint": "tsc --noEmit && eslint --ext .ts,.js src --ignore-path ../../.gitignore",
        "setup": "bun run scripts/codegen.ts"
    },
    "dependencies": {
        "@atproto/api": "^0.13.27",
        "@atproto/identity": "^0.4.5",
        "@atproto/lexicon": "^0.4.5",
        "@atproto/xrpc": "^0.6.6"
    },
    "devDependencies": {
        "@atcast/eslint-config": "workspace:*",
        "@atcast/prettier-config": "workspace:*",
        "@atproto/lex-cli": "^0.5.5",
        "@types/bun": "^1.1.16",
        "tsup": "^8.3.5",
        "typescript": "^5.7.3"
    }
}
